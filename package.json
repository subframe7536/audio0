{
  "name": "audio0",
  "type": "module",
  "version": "0.1.0",
  "description": "Audio library for the modern browser, auto cross fade",
  "author": "subframe7536",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subframe7536/audio0.git"
  },
  "keywords": [
    "audio",
    "sound",
    "web audio",
    "html5 audio"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "release": "pnpm run build && bumpp --all && npm publish",
    "format": "eslint . --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@subframe7536/type-utils": "^0.1.6",
    "zen-mitt": "^3.0.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.9.1",
    "bumpp": "^9.5.2",
    "eslint": "^9.10.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "weighted-shuffle": "^1.0.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "weighted-shuffle@1.0.0": "patches/weighted-shuffle@1.0.0.patch"
    }
  }
}
